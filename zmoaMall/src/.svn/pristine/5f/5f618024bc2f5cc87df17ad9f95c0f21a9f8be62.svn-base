package com.zm.mall.common.utils;

/**查询订单
 * Created by lp on 2016/12/2.
 */
public class OrderBySku {
    private  String productCode;
    private String productName;
    private String searchProductCategories;
    private Integer typeId;
    private Long bmId;
    private Long bmValue;
    private Long hdId;
    private Long hdValue;
    private Integer beginNumber;
    private Integer pageSize;
    private String pluteformid;

    public String getPluteformid() {
        return pluteformid;
    }

    public void setPluteformid(String pluteformid) {
        this.pluteformid = pluteformid;
    }

    public OrderBySku() {
    }

    public String getProductCode() {
        return productCode;
    }

    public void setProductCode(String productCode) {
        this.productCode = productCode;
    }

    public String getProductName() {
        return productName;
    }

    public void setProductName(String productName) {
        this.productName = productName;
    }

    public String getSearchProductCategories() {
        return searchProductCategories;
    }

    public void setSearchProductCategories(String searchProductCategories) {
        this.searchProductCategories = searchProductCategories;
    }

    public Integer getTypeId() {
        return typeId;
    }

    public void setTypeId(Integer typeId) {
        this.typeId = typeId;
    }

    public Long getBmId() {
        return bmId;
    }

    public void setBmId(Long bmId) {
        this.bmId = bmId;
    }

    public Long getBmValue() {
        return bmValue;
    }

    public void setBmValue(Long bmValue) {
        this.bmValue = bmValue;
    }

    public Long getHdId() {
        return hdId;
    }

    public void setHdId(Long hdId) {
        this.hdId = hdId;
    }

    public Long getHdValue() {
        return hdValue;
    }

    public void setHdValue(Long hdValue) {
        this.hdValue = hdValue;
    }

    public Integer getBeginNumber() {
        return beginNumber;
    }

    public void setBeginNumber(Integer beginNumber) {
        this.beginNumber = beginNumber;
    }

    public Integer getPageSize() {
        return pageSize;
    }

    public void setPageSize(Integer pageSize) {
        this.pageSize = pageSize;
    }
}
