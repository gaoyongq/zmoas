package com.zm.mall.domain.system;

import java.math.BigDecimal;

/**
 * 费用的详细明细  以后费用明细可以进行定制。
 * @author Administrator
 *
 */
public class MoneyItem {

	private Long id ;

	//费用的名称
	private String moneyName;
	//code
	private String code;
	//费用的事由
	private String description;

	//所在的费用名单
	private  Long moneyManageId;

	//金额
	private BigDecimal money;



	public String getMoneyName() {
		return moneyName;
	}

	public void setMoneyName(String moneyName) {
		this.moneyName = moneyName;
	}

	public String getCode() {
		return code;
	}

	public void setCode(String code) {
		this.code = code;
	}

	public String getDescription() {
		return description;
	}

	public void setDescription(String description) {
		this.description = description;
	}

	public Long getMoneyManageId() {
		return moneyManageId;
	}

	public void setMoneyManageId(Long moneyManageId) {
		this.moneyManageId = moneyManageId;
	}

	public BigDecimal getMoney() {
		return money;
	}

	public void setMoney(BigDecimal money) {
		this.money = money;
	}

	public Long getId() {
		return id;
	}

	public void setId(Long id) {
		this.id = id;
	}

}
