        </if>
        <if test="pluteformid != null and pluteformid != ''">
            AND o.pluteformid = #{pluteformid}
        </if>
    </select>
    <!--月金额量-->
    <select id="orderTotal" resultType="double">
        SELECT SUM(o.orderTotal) FROM b2c_shop_orders o
        WHERE 1=1
        <if test="startTime != null and startTime != ''">
            AND o.createdDate &gt;= #{startTime}
        </if>
        <if test="endTime != null and endTime != ''">
            AND o.createdDate &lt;= #{endTime}
        </if>
        <if test="pluteformid != null and pluteformid != ''">
            AND o.pluteformid = #{pluteformid}
        </if>
    </select>
    <!--商城下的店铺订单销量-->
    <select id="emporiumShopChampionHip" resultMap="OrderInfoBaseMap" >
        SELECT o.orderId,o.orderCode,o.createdDate,i.orderId,i.orderCode,i.productid,i.name,SUM(i.quantity) AS quantity,o.pluteformid,i.sellPrice
        FROM b2c_shop_orders o,b2c_shop_orderitems i
        WHERE o.orderCode=i.orderCode
        AND i.ProductId IN
        <foreach item="item" collection="productTds" separator="," open="(" close=")">
            #{item}
        </foreach>
        <if test="startTime != null and startTime != ''">
            AND o.createdDate &gt;= #{startTime}
        </if>
        <if test="endTime != null and endTime != ''">
            AND o.createdDate &lt;= #{endTime}
        </if>
        <if test="pluteformid != null and pluteformid != ''">
            AND o.pluteformid = #{pluteformid}
        </if>
        GROUP BY i.productid
        ORDER BY quantity DESC
        Limit #{begionNumber},#{pageSize}
    </select>
    <select id="emporiumShopChampionHipCount" resultType="int">
        SELECT COUNT(DISTINCT(i.productid))
        FROM b2c_shop_orders o,b2c_shop_orderitems i
        WHERE o.orderCode=i.orderCode
        AND i.productid IN
        <foreach item="item" collection="productTds" separator="," open="(" close=")" >
            #{item}
        </foreach>
        <if test="startTime != null and startTime != ''">
            AND o.createdDate &gt;= #{startTime}
        </if>
        <if test="endTime != null and endTime != ''">
            AND o.createdDate &lt;= #{endTime}
        </if>
        <if test="pluteformid != null and pluteformid != ''">
            AND o.pluteformid = #{pluteformid}
        </if>
    </select>
    <!--店铺订单量-->
    <select id="shopOrderQuantity" resultType="int">
        SELECT COUNT(DISTINCT(o.orderCode))
        FROM b2c_shop_orders o,b2c_shop_orderitems i
        WHERE o.orderCode=i.orderCode
        AND i.productid IN
        <foreach item="item" collection="productTds" separator="," open="(" close=")" >
            #{item}
        </foreach>
        <if test="startTime != null and startTime != ''">
            AND o.createdDate &gt;= #{startTime}
        </if>
        <if test="endTime != null and endTime != ''">
            AND o.createdDate &lt;= #{endTime}
        </if>
        <if test="pluteformid != null and pluteformid != ''">
            AND o.pluteformid = #{pluteformid}
        </if>
    </select>
    <!--店铺订单金额-->
    <select id="shopOrderTotal" resultMap="OrderInfo">
        SELECT DISTINCT(o.orderCode),SUM(o.orderTotal) ordertotal
        FROM b2c_shop_orders o,b2c_shop_orderitems i
        WHERE o.orderCode=i.orderCode
        AND i.productid IN
        <foreach item="item" collection="productTds" separator="," open="(" close=")" >
            #{item}
        </foreach>
        <if test="startTime != null and startTime != ''">
            AND o.createdDate &gt;= #{startTime}
        </if>
        <if test="endTime != null and endTime != ''">
            AND o.createdDate &lt;= #{endTime}
        </if>
        <if test="pluteformid != null and pluteformid != ''">
            AND o.pluteformid = #{pluteformid}
        </if>
    </select>
    <select id="selUserOrderTotal" resultType="double">
        SELECT SUM(o.ordertotal)
        FROM b2c_shop_orders o
        WHERE buyerid=#{users.userId}
        <if test="startTime != null and startTime != ''">
            AND o.createdDate &gt;= #{startTime}
        </if>
        <if test="endTime != null and endTime != ''">
            AND o.createdDate &lt;= #{endTime}
        </if>
    </select>
    <select id="selectOneOrderByOrderCode" resultMap="OrderInfoBaseMap" parameterType="orderInfo">
      SELECT      *
        FROM b2c_shop_orders o,b2c_shop_orderitems i
        WHERE
        o.orderCode=i.orderCode
        AND
        o.orderCode=#{orderCode}
    </select>
    <select id="getYUYUEOrderInfo" parameterType="page" resultMap="OrderInfoBaseMap">
        select  * from b2c_appointment_order orders
        INNER JOIN b2c_shop_orderitems  item on orders.OrderId=item.OrderId
        where orders.OrderStatus=#{orderInfo.orderStatus} AND orders.BuyerName=#{orderInfo.buyerName} and orders.pluteformid=#{orderInfo.pluteformid}
        limit #{beginNumber},#{pageSize}
    </select>
</mapper>
