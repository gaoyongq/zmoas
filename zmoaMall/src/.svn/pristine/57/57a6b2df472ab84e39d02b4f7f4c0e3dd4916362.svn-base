public enum BaseModuleEnum {
    NAVIGATION_MODULE(1,"导航模块"),
    BANNER_MODULE(2,"轮播图模块"),
    CATEGORY_RECOMMEND_MODULE(3,"分类推荐模块"),
    ITEM_RECOMMEND_MODULE(4,"商品推荐模块"),
    NOTICE_MODULE(5,"公告模块"),
    USER_DEFINED_MODULE(6,"自定义模块"),
    LOGO(7,"LOGO"),
    CATEGORY(8,"类目"),
    FOOT_NOTICE(9,"页脚"),
    ITEM_LIST(10,"商品列表"),
    ITEM_RANK(12,"销量排行模块"),
    SELLER_CLASS(13,"商品分类"),
    IMAGE_EDIT(14,"图片编辑"),
    FLOOR_EDIT(15,"楼层模块"),
    FONT_EDIT(16,"文字编辑"),
    LOGIN_EDIT(17,"登录"),
    SELLER_INFO(11,"商家信息"),
    FONT_FLOOR_MODULE(19,"楼层文字标题"),
    IMAGE_FLOOR_MODULE(20,"楼层图文标题"),
    CUSTOM_MODULE(21,"自定义模块");

    private int type;

    private String desc;

    BaseModuleEnum(int type, String desc) {
        this.type = type;
        this.desc = desc;
    }

    public int getType() {
        return type;
    }

    public void setType(int type) {
        this.type = type;
    }

    public String getDesc() {
        return desc;
    }

    public void setDesc(String desc) {
        this.desc = desc;
    }
}
